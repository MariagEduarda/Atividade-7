programa
{
	

	funcao inicio()

	{
	   inteiro soma = 0
        inteiro numeros[3][3] = {
            {1,4,7},
            {2,5,8},
            {3,6,9}
        }
        
        para(inteiro i = 0; i< 3; i++){
            para(inteiro c = 0; c < 3; c++){
                soma = soma + numeros[i][c]
            }
        }
        
        escreva("A soma Ã©: ", soma)
	}
}
