programa
{

	funcao inicio()
	{
		inteiro valor[11] 

		escreva("Escreva números inteiros \n")
		para(inteiro i = 1; i < 11; i++){
			escreva("Escreva o ", i ," número: ")
			leia(valor[i])
			
			se (valor[i] %2 == 0){
           	escreva( "a linha ", i , " com o número " ,(valor[i]) , " é par \n ")
           	}
          
          
           }

	}
}
